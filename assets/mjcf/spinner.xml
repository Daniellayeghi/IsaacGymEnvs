<mujoco model="finger">
  <custom>
    <numeric data="0.0" name="init_qpos"/>
  </custom>

  <compiler inertiafromgeom="true" angle="degree"/>

  <option timestep="0.01" collision="all"/>

    <worldbody>
        <light name="light" directional="true" diffuse=".6 .6 .6" pos="0 0 2" specular=".3 .3 .3"/>
        <geom name="ground" type="plane" pos="0 0 -0.6" size=".6 .2 10"  solimp="0 0.9 0.01" solref=".02 1"/>
        <camera name="cam0" pos="0 -1 .8" xyaxes="1 0 0 0 1 2"/>
        <camera name="cam1" pos="0 -1 .4" xyaxes="1 0 0 0 0 1" />

        <body name="spinner_root">
            <body name="spinner" pos="0 0 -.4">
                <joint name="spinner_joint" type="hinge" axis="0 -1 0" range="-50 50" frictionloss=".01" damping=".35" limited="false" />
                <geom name="cap1" type="capsule" size=".04 .09" pos=".02 0 0" solimp="0 0.9 0.01" solref=".02 1" condim="1"/>
                <geom name="cap2" type="capsule" size=".04 .09" pos="-.02 0 0" solimp="0 0.9 0.01" solref=".02 1" condim="1"/>
                <geom name="spinner_decoration" type="cylinder" fromto="0 -.045 0 0 .045 0" size=".02" solimp="0 0.9 0.01" solref=".02 1" condim="1"/>
            </body>
        </body>
    </worldbody>
</mujoco>
